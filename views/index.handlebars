<!DOCTYPE html>
<html lang="en">
	{{> header}}
</head>
<body>
	{{> nav}}
	<div id="audio-player" class="audio-player-wrapper">
 		<div id="bg" class="audio-player-image"></div>
		<nav id="playerNav">
			<ul class="audio-player-controls">
				<li class="green">
					<a role="button" class="audio-player-button icon-backward"></a>
				</li>
				<li class="red">
					<a role="button" class="audio-player-button audio-player-place-pause-button icon-play"></a>
				</li>
				<li class="blue">
					<a role="button" class="audio-player-button icon-forward"></a>
				</li>
			</ul>
		</nav>
	</div>
	<script type='text/javascript' src="js/audio.js"></script> 
</body>
</html>